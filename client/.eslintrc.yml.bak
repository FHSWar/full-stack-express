env:
  browser: true
  es2021: true
extends:
  - plugin:@typescript-eslint/recommended
  - plugin:vue/vue3-recommended
  - standard-with-typescript
parserOptions:
  parser: '@typescript-eslint/parser'
  sourceType: module
plugins:
  - '@typescript-eslint'
  - vue
rules: {
  '@typescript-eslint/triple-slash-reference': off
}
